{
  "name": "bventy-monorepo",
  "private": true,
  "packageManager": "npm@11.10.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "turbo": "^2.4.4",
    "prettier": "^3.5.2",
    "typescript": "^5.8.2"
  }
}